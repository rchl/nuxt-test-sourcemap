    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "Debug SSR",
        "restart": true,
        "protocol": "inspector",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceRoot}/app/index.js",
        "env": {
            "ENV": "development",
        },
        "sourceMaps": true,
        "stopOnEntry": true,
    }]
}
